<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.algogame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AlgoGame">
        <activity android:name=".getName"></activity>
        <activity android:name=".homescreen" />
        <activity android:name=".Sort_Game" />
        <activity android:name=".Queue_Game" />
        <activity android:name=".Stack_Game" />
        <activity android:name=".PushandPop_Stack_Tutorial" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>